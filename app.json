{
  "expo": {
    "owner": "aramaysm",
    "name": "SCA-QR-Scanner",
    "slug": "qrcode-scanner-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "blockedPermissions": [
          "android.permission.CAMERA"
        ]
      },
      "package": "com.accesscontrolsecurity.scaqrcodescan",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"      
      
    }
    
  }
}


